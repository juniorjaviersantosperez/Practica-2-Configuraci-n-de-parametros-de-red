ls
cd Desktop/
mkdir materia
ls
cd materia/
touch estudiante.txt
vi estudiante.txt
cat estudiante.txt
sudo chmod estudiante.txt
sudo chmod 700 estudiante.txt
ls -l estudiante.txt
sudo chmod 070 estudiante.txt
ls -l estudiante.txt
cd ..
mkdir materia2
cp materia/estudiante.txt materia2
sudo chmod 700 estudiante.txt
ls
cd materia
ls
sudo chmod 700 estudiante.txt
ls -l estudiante.txt
cp materia/estudiante.txt materia2
cd ..
cp materia/estudiante.txt materia2
ls
cd materia2
ls
cd ..
rm -r materia
rm -r materia2
clear
